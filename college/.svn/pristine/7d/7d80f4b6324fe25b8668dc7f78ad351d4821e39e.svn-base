<!-- header -->
<div class="header container-fluid">
  <div class="container">
    <div class="header-header col-xs-12"></div>
    <div class="col-xs-6 header-logo-name">
      <img ui-sref="main.homepage" class="header-logo" src="imgs/logo.png" alt="学校logo">
      <img class="header-name" src="imgs/header-name.png" alt="slogan">
    </div>
    <div class="col-xs-6 header-slogan-search">
      <img class="header-slogan" src="imgs/slogan.png" alt="" >
      <div class="header-search text-right">
        <a class="set-homepage" href="javascript:void(0)" homepage>设为首页</a>
        <form class="input-normal" action="">
            <a href="">
              <span class="icon-search"></span>
            </a>
          <label for="">
            <input  type="text">
          </label>
        </form>
      </div>
    </div>
  </div>
</div>
<div class="container">
  <div class="row">
    <ul class="col-xs-12 nav-list clear-padding" nav>
      <li>
        <a ng-class="{'active':vm.state.includes('main.homepage')}" ui-sref="main.homepage">首页</a>
      </li>
      <li><a ng-class="{'active':vm.state.includes('main.specialtyIntroduce')}"  ui-sref="main.specialtyIntroduce">专业介绍</a></li>
      <li><a ng-class="{'active':vm.state.includes('main.faculty')}" ui-sref="main.faculty.facultyContent({teacherId:0})">师资团队</a></li>
      <li><a ng-class="{'active':vm.state.includes('main.practiceBase')}" ui-sref="main.practiceBase">实习基地</a></li>
      <li><a ng-class="{'active':vm.state.includes('main.communication')}" ui-sref="main.communication">学术交流</a></li>
      <li><a ng-class="{'active':vm.state.includes('main.events')}"  ui-sref="main.events">学生活动</a></li>
      <li><a ng-class="{'active':vm.state.includes('main.recruit')}" ui-sref="main.recruit.recruitContent({id:0})">招生就业</a></li>
      <li><a ng-class="{'active':vm.state.includes('main.outstandingGraduates')}" ui-sref="main.outstandingGraduates">优秀毕业生</a></li>
      <li><a ng-class="{'active':vm.state.includes('main.contactUs')}" ui-sref="main.contactUs">联系我们</a></li>
      <span></span>
    </ul>
  </div>
</div>
<div ui-view="" class="main"></div>
