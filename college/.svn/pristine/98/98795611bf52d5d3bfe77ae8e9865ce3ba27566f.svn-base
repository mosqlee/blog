.border {
  border: 1px solid #e8e8e8;
}
.container {
  padding: 0!important;
  width: 1200px!important;
}
.row {
  margin: 0;
}
.ddgray {
  color: #666666;
}
a:hover {
  cursor: pointer;
  color: #3d85d6;
  text-decoration: none;
}
a:focus {
  text-decoration: none;
  outline: none;
}
.clear-left-padding {
  padding-left: 0;
}
.clear-right-padding {
  padding-right: 0;
}
.clear-padding {
  padding-left: 0;
  padding-right: 0;
}
.border-top-homepage {
  border-top: 5px solid #3d85d6;
}
.specialty-banner {
  width: 1200px;
  height: 150px;
}
.specialty-nav {
  margin: 20px;
}
img {
  width: 100%;
  height: 100%;
}
.specialty-content {
  margin-top: 60px;
  margin-bottom: 190px;
}
.specialty-content-title {
  padding-left: 10px ;
  border-left: 4px solid #3d85d6;
}
.faculty-list-tittle {
  display: inline-block;
  padding-left: 40px;
  width: 200px;
  height: 45px;
  background: #3d85d6 ;
  line-height: 45px;
}
.faculty-list-content {
  display: inline-block;
  margin: -1px 0;
  padding-left: 40px;
  width: 200px;
  min-height: 45px;
  line-height: 45px;
  border: 1px solid #e8e8e8;
  border-top: 0;
}
.faculty-list-content:hover {
  color: #3d85d6;
  background: #e2dede;
}
.faculty-content {
  margin-bottom: 90px;
}
.selected {
  color: #3d85d6;
  background: #e3e3e3;
}
.selected2 {
  font-weight: 700;
  background: #e3e3e3;
}
.selected3 {
  background: #ffffff;
}
.base-content {
  margin-top: 60px;
  width: 1200px;
}
.base-pic-warp {
  margin-left: 30px;
  width: 300px;
  height: 230px;
}
.communication-pic {
  margin-bottom: 10px;
  width: 290px;
  height: 155px;
}
.communication-content {
  display: inline-block;
  margin: 20px 0;
  height: 300px;
}
.communication-title {
  position: relative;
  display: table;
  margin-bottom: 10px;
  overflow: hidden;
}
.communication-border {
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.communication-date {
  float: left;
  z-index: 100;
  margin-right: 10px;
  font-size: 24px;
  text-align: center;
}
.communication-date-date {
  width: 66px;
  height: 44px;
  line-height: 44px;
  border-bottom: solid 1px #ffffff;
}
.communication-date-year {
  padding-top: 4px;
  height: 24px;
}
.communication-title-content {
  display: table-cell;
  top: 50%;
  width: 215px;
  vertical-align: middle;
}
.communication-title-content:hover {
  color: #3d85d6;
}
.communication-text {
  display: inline-block;
  width: 290px;
}
.communication-text:hover {
  color: #3d85d6;
}
.communicationContent {
  margin-top: 40px;
  margin-bottom: 90px;
}
.outstandingGraduates-table {
  display: inline-block;
}
.outstandingGraduates-content {
  border: 1px solid #666666;
}
.outstandingGraduates-content-title {
  display: inline-block;
  padding-left: 25px;
  min-width: 175px;
  height: 40px;
  line-height: 40px;
  border-right: 1px solid #666666;
}
.outstandingGraduates-content-content {
  display: inline-block;
  padding-left: 10px;
  min-width: 255px;
  height: 40px;
  line-height: 40px;
}
.contactUs-content {
  display: inline-block;
  margin-bottom: 90px;
  width: 1200px;
  height: 510px;
  background: #3d85d6;
}
.contactUs-content-list {
  position: relative;
  float: left;
  width: 260px;
}
.contactUs-content-list-title {
  margin: 60px 0;
  font-size: 18px;
}
.contactUs-content-list-content {
  margin-bottom: 45px;
  margin-left: 80px;
}
.contactUs-content-list-phone {
  position: absolute;
  top: 130px;
  left: 36px;
  height: 39px;
  width: 27px;
  background-image: url("../imgs/contactUsSprite.png");
  background-position: -4px 0;
}
.contactUs-content-list-net {
  position: absolute;
  top: 205px;
  left: 36px;
  height: 30px;
  width: 36px;
  background-image: url("../imgs/contactUsSprite.png");
  background-position: 0 -39px;
}
.contactUs-content-list-letter {
  position: absolute;
  top: 265px;
  left: 36px;
  height: 37px;
  width: 36px;
  background-image: url("../imgs/contactUsSprite.png");
  background-position: -1px -73px;
}
.contactUs-content-list-address {
  position: absolute;
  top: 335px;
  left: 36px;
  height: 42px;
  width: 38px;
  background-image: url("../imgs/contactUsSprite.png");
  background-position: 0 -110px;
}
.contactUs-content-map {
  display: inline-block;
  width: 940px;
  margin-top: 4px;
}
.pages {
  display: inline-block;
  margin-top: 20px;
  margin-bottom: 95px;
}
.page-btn {
  padding: 0 14px;
  height: 38px;
  border: 1px solid #d8d8d8;
  border-radius: 0;
}
.page-next {
  height: 38px;
  width: 76px;
  background: #eef6ff;
  border: 1px solid #e8e8e8;
  border-radius: 0;
}
.page-input {
  height: 38px;
  width: 38px;
  border: 1px solid #d8d8d8;
  outline: none;
}
