app.factory('getUrl',function(){
  return {
    getUrl:
    function (p,c){
      var url = "";
      switch (p) {
        case 0:
          url = ".faculty.facultyContent";
          break;
        case 1:
          url = ".outstandingGraduates";
          break;
        case 2:
          url = ".communicationContent";
          break;
        case 3:
          url = ".eventsContent";
          break;
        case 4:
          url = ".recruit.recruitContent";
          break;
        case 5:
          url = ".recruit.recruitContent";
          break;
        case 6:
          url = ".practiceBase";
          break;
        case 7:
          url = ".specialtyIntroduce";
          break;
        case 8:
          url = ".homepage";
          break;
        default:
          alert("未找到url");
          break;
      }
      return p = "main"+url;
    }
  }
})